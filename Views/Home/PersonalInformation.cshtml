@section scripts{
    <script src="@Url.Content("~/Scripts/jquery.validate.js")"></script>
    <script src='https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit' async defer></script>
    <script>
        $(document).ready(function () {
            $('#ConfirmEmail').bind("cut copy paste", function (e) {
                e.preventDefault();
            });
            $('#email').bind("cut copy paste", function (e) {
                e.preventDefault();
            });
            $('#contactNumber').bind("cut copy paste", function (e) {
                e.preventDefault();
            });

        });
        $("#contactNumber").keydown(function (e) {
            // Allow: backspace, delete, tab, escape, enter and .
            if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 190]) !== -1 ||
                // Allow: Ctrl+A
                (e.keyCode == 65 && e.ctrlKey === true) ||
                // Allow: home, end, left, right
                (e.keyCode >= 35 && e.keyCode <= 39)) {
                // let it happen, don't do anything

                return;
            }
            // Ensure that it is a number and stop the keypress
            if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                e.preventDefault();
            }
            
        });
        $("#FirstName").keydown(function (e) {
            var specialKeys = new Array();
            specialKeys.push(8); //Backspace
            specialKeys.push(9); //Tab
            specialKeys.push(46); //Delete
            specialKeys.push(36); //Home
            specialKeys.push(35); //End
            specialKeys.push(37); //Left
            specialKeys.push(39); //Right
            var keyCode = e.keyCode == 0 ? e.charCode : e.keyCode;
            
            var ret = ((e.which>32 && e.which<48)||(keyCode >= 65 && keyCode <= 90) || (keyCode >= 97 && keyCode <= 122) || (specialKeys.indexOf(e.keyCode) != -1 && e.charCode != e.keyCode));
            if (!ret) {
                e.preventDefault();
            }
            if (keyCode == 111 || keyCode == 106 || keyCode == 109 || keyCode == 107) {
                e.preventDefault();
            }
        });

        $("#LastName").keydown(function (e) {
            var specialKeys = new Array();
            specialKeys.push(8); //Backspace
            specialKeys.push(9); //Tab
            specialKeys.push(46); //Delete
            specialKeys.push(36); //Home
            specialKeys.push(35); //End
            specialKeys.push(37); //Left
            specialKeys.push(39); //Right
            var keyCode = e.keyCode == 0 ? e.charCode : e.keyCode;

            var ret = ((e.which > 32 && e.which < 48) || (keyCode >= 65 && keyCode <= 90) || (keyCode >= 97 && keyCode <= 122) || (specialKeys.indexOf(e.keyCode) != -1 && e.charCode != e.keyCode));
            if (!ret) {
                e.preventDefault();
            }
            if (keyCode == 111 || keyCode == 106 || keyCode == 109 || keyCode == 107) {
                e.preventDefault();
            }
        });

        $('.black-but-big').click(function () {

            var validator = $("#personalInformation").validate({
                ignore: [],
                rules: {
                    FirstName: "required",
                    LastName: "required",
                    PostalCode: "required",
                    MobileNo: { required: true, maxlength: 20, minlength: 6 },
                    Email: { required: true, email: true },
                    ConfirmEmail: { equalTo: "#email", required: true },
                    Country: "required",
                    Address: "required",
                    captchaResponse: "required"
                },
                messages: {
                    Email: {
                        required: "Please enter email.",
                        email: "Please enter valid email."
                    },
                    ConfirmEmail: {
                        equalTo: "Please enter confirm email same as email.",
                        required: "Please enter confirm email.",
                        email: "Please enter valid email."
                    },
                    Country: "Please select country.",
                    FirstName: "Please enter First Name.",
                    LastName: "Please enter Last Name.",
                    PostalCode: "Please select country code.",
                    MobileNo: {
                        required: "Please enter contact number.",
                        minlength: "Please enter 6-20 digits for mobile.",
                        maxlength: "Enter max 20 digits."

                    },
                    Address: "Please enter home address.",
                    captchaResponse: "Please tick on  i'm not a robot."
                },
                errorPlacement: function (error, element) {
                    error.insertAfter(element.parent("div")).wrap('<label/>');
                    $('label.error').css('color', '#ff0000');
                }
            });

            var valid = validator.form();
            var CaptchaResponse = null;
            CaptchaResponse = grecaptcha.getResponse(widgetId1);
            if (valid && CaptchaResponse != null && CaptchaResponse != "") {
                $("#personalInformation").submit();
            }
        });

        var widgetId1;
        var onloadCallback = function () {
            $("#captchaResponse").val("");
            var S_Key = '@System.Configuration.ConfigurationManager.AppSettings["captcha_sitekey"]';
            widgetId1 = grecaptcha.render('captcha', {
                'sitekey': S_Key,//'@System.Configuration.ConfigurationManager.AppSettings["captcha_sitekey"]',//'6LfyHRwTAAAAAEH1xPc1ZsmxE3rSwXOc6fy1MD-i',
                'theme': '@System.Configuration.ConfigurationManager.AppSettings["captcha_theme"]',
                'callback': verifyCallback,
            });
        };

        var verifyCallback = function (response) {
            $("#captchaResponse").val("");
            response1 = response;
            $("#captchaResponse").val(response);
        };
        $('#code').change(function () {
            var code = ($('#code option:selected').text().split('(')[0]).trim();
            //alert(code.split('(')[0]);
            if (code != "Other") {
                $("#country option").filter(function () {
                    return this.text == code.split('(')[0];
                }).attr('selected', true);
            }
            else {
                $("#country option").filter(function () {
                    return this.text == "Select country";
                }).attr('selected', true);
            }
        });
    </script>
